{
    "extends": "airbnb-base",
    "env": {
        "mocha": true,
        "es6": true,
        "node": true
    },
    "parserOptions": { 
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "arrowFunctions": true
        }
     },
     "rules": {
         "no-extend-native": "off"
     }
}